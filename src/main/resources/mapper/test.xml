<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mcnc.practice.test">

    <insert id="register">
        INSERT INTO TEST (
              ID
            , NAME
        ) VALUES (
              #{id}
            , #{name}
        )
    </insert>

</mapper>